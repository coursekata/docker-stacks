#!/bin/bash

# Check for correct number of arguments
if [ "$#" -ne 2 ]; then
    echo "Usage: $0 <previous-libs-file> <current-libs-file>"
    exit 1
fi

# Input file paths for the lists of shared libraries to compare
PREVIOUS_LIBS_FILE=$1
CURRENT_LIBS_FILE=$2

# Output new shared libraries that are in the current list but not in the previous one
comm -13 <(sort ${PREVIOUS_LIBS_FILE}) <(sort ${CURRENT_LIBS_FILE})
